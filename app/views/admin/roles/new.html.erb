<%= form_with url: admin_roles_path, model: @role, local: true do |f| %>
<div class="dashboard">
  <div class="toolbar dashboard-toolbar">
    <div class="nav">
      <%= render_breadcrumbs %> / <span>New Role</span>
    </div>
    <div></div>
    <%= f.submit 'Save', class: 'button button-solid is-green' %>
  </div>

  <div class="dashboard-form">
    <%= render partial: 'admin/roles/form', locals: { f: f } %>
  </div>
</div>
<% end %>
