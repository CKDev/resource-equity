<%= form_with url: admin_token_path(@token), model: @token, local: true do |f| %>
<div class="dashboard">
  <div class="toolbar dashboard-toolbar">
    <div class="nav">
      <%= render_breadcrumbs %> / <span><%= @token.title %></span>
    </div>
    <div></div>
    <%= f.submit 'Save', class: 'button button-solid is-green' %>
  </div>

  <div class="dashboard-form">
    <%= render partial: 'admin/tokens/form', locals: { f: f } %>
  </div>
</div>
<% end %>
